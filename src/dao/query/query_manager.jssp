query insertManager(managerAge Age, managerName Name) Long {

    INSERT INTO
        manager
        (
            manager_age,
            manager_name
        )
    VALUES
        (
            :managerAge,
            :managerName
        )

}

query selectManagerByManagerId(managerId Long) ManagerInfoVo {

    SELECT
        manager_id,
        manager_name,
        manager_age
    FROM
        manager
    WHERE
        manager_id = :managerId

}