 query insertDormitory(dormitoryName Name, dormitoryAddress Address) Long {
    
    INSERT INTO 
        dormitory
        (
            dormitory_name, 
            dormitory_address
        ) 
    VALUES 
        (
            :dormitoryName,
            :dormitoryAddress
        )

 }

