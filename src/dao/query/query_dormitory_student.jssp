query insertDormitoryStudentInRoom(dormitoryId Long, roomId Long, studentId Long, dormitoryStudentSemester TypeID) Long {

    INSERT INTO
        dormitory_student
        (
            dormitory_id,
            room_id,
            student_id,
            dormitory_student_semester
        )
    VALUES
        (
            :dormitoryId,
            :roomId,
            :studentId,
            :dormitoryStudentSemester
        )

}

